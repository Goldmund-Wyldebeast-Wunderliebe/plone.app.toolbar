<?xml version="1.0"?>
<registry>

    <record name="plone.app.toolbar.skinname">
        <field type="plone.registry.field.TextLine">
            <title>Toolbar Skin</title>
        </field>
        <value>toolbar</value>
    </record>

    <record
        name="Products.ResourceRegistries.interfaces.settings.IResourceRegistriesSettings.resourceBundlesForThemes">
        <value purge="false">
            <element key="toolbar">
                <element>old</element>
                <element>tinymce</element>
                <element>jquery</element>
                <element>bootstrap</element>
                <element>toolbar</element>
            </element>

            <element key="Plone Classic Theme">
                <element>old</element>
                <element>tinymce</element>
                <element>jquery</element>
                <element>default</element>
                <element>toolbar-init</element>
            </element>

            <element key="Plone Default">
                <element>old</element>
                <element>tinymce</element>
                <element>jquery</element>
                <element>default</element>
                <element>toolbar-init</element>
            </element>

            <element key="Sunburst Theme">
                <element>old</element>
                <element>tinymce</element>
                <element>jquery</element>
                <element>default</element>
                <element>toolbar-init</element>
            </element>
        </value>
    </record>

</registry>
